  <h2>Events List</h2>
  <table class="table table-striped table-bordered table-hover">
    <thead class="thead-dark">
      <tr>
        <th>Name</th>
        <th>Number of Participants</th>
        <th>Date</th>
        <th>Time</th>
      </tr>
    </thead>

    <tbody>
      <% @events.each do |event| %>

        <tr>
          <td><%= event.name%></td>          
          <td><%= event.no_of_participant %></td>
          <td><%= event.date %></td>
          <td><%= event.time.strftime('%H:%M:%S') %></td>  
        </tr>

      <% end %>
    </tbody>
  </table>
